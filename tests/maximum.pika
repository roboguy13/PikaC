%generate max
-- %generate maximum

Sll : layout[-x](List);
Sll Nil := emp;
Sll (Cons head tail) :=
  exists +nxt.
  x :-> head **
  (x+1) :-> nxt **
  Sll tail [nxt];

max : Int -> Int -> Int;
max x y | x < y := y;
max x y | not (x < y) := x;

