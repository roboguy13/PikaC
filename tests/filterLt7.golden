void filterLt7(loc x, loc x1);
void filterLt7(loc x, loc x1) {
 loc x2 = READ_LOC(x1, 0);
 if (x < 7) {
  if (x == 0) {
   WRITE_LOC(x1, 0, 0);
  } else {
   loc h = READ_LOC(x, 0);
   loc n = READ_LOC(x, 1);
   if (h < 7) {
    filterLt7(n, x1);
   } else {
    if (n < 7) {
     filterLt7(n, x1);
     WRITE_LOC(x, 0, n);
    } else {
     filterLt7(n, x1); WRITE_LOC(x, 0, x);
    }
   }
  }
 } else {
  if (x1 < 7) {
   loc h = READ_LOC(x, 0);
   loc n = READ_LOC(x, 1);
   if (h < 7) {
    filterLt7(n, x1);
   } else {
    if (n < 7) {
     filterLt7(n, x1);
     WRITE_LOC(x, 0, n);
    } else {
     if ((n <= x) && (x <= n)) {
      filterLt7(n, x1);
      free( x );
     } else {
      filterLt7(n, x1); WRITE_LOC(x, 0, x1);
     }
    }
   }
  } else {
   if (x2 < 7) {
    loc h = READ_LOC(x, 0);
    loc n = READ_LOC(x, 1);
    if (h < 7) {
     filterLt7(n, x1);
    } else {
     if (n < 7) {
      filterLt7(n, x1);
      WRITE_LOC(x, 0, n);
     } else {
      if ((n <= x) && (x <= n)) {
       filterLt7(n, x1);
       free( x );
      } else {
       filterLt7(n, x1); WRITE_LOC(x, 0, x2);
      }
     }
    }
   } else {
    loc h = READ_LOC(x, 0);
    loc n = READ_LOC(x, 1);
    if (h < 7) {
     filterLt7(n, x1);
    } else {
     if (n < 7) {
      filterLt7(n, x1);
      WRITE_LOC(x, 0, n);
     } else {
      if ((n <= x) && (x <= n)) {
       filterLt7(n, x1);
       free( x );
      } else {
       filterLt7(n, x1); WRITE_LOC(x, 0, 0);
      }
     }
    }
   }
  }
 }
