%generate append

Sll : layout[x](List);
Sll Nil := emp;
Sll (Cons head tail) :=
  x :-> head **
  (x+1) :-> nxt **
  Sll tail [nxt];

append : Sll -> Sll -> Sll;
append Nil Nil := Nil [Sll];
append Nil (Cons h t) := Cons h t [Sll];
append (Cons h t) Nil := Cons h t [Sll];
append (Cons h t) (Cons h2 t2) :=
  Cons h (append t (Cons h2 t2 [Sll])) [Sll];

